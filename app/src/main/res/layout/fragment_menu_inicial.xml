<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".presenter.initial.menuinicial.MenuInicialFragment">

    <TextView
        android:id="@+id/text_view_principal"
        style="@style/text_view_title_list"
        android:text="@string/texto_menu_principal" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/list_menu_inicial"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        tools:listitem="@layout/item_row_text" />

    <TextView
        android:id="@+id/text_view_processo"
        style="@style/text_view_item_list"
        tools:text="TESTE" />

</LinearLayout>